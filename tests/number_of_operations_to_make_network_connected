#[cfg(test)]
mod number_of_operations_to_make_network_connected {
    use solver::number_of_operations_to_make_network_connected::make_connected;

    #[test]
    fn example1() {
        assert_eq!(
            make_connected(4, vec![vec![0, 1], vec![0, 2], vec![1, 2]]),
            1
        );
    }
}
